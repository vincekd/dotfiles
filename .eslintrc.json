{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {},
    "rules": {
        "no-console": 0,
        "indent": [
            "warn",
            4,
            {"ArrayExpression": "first"}
        ],
        "linebreak-style": [
            "warn",
            "unix"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-extra-boolean-cast": 0
    }
}
